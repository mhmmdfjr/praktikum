#include <iostream>

using namespace std;
void tukar(int a, int b);

void tukar (int a, int b) {
     int c;
     c = a;
     a = b;
     b = c;
     
     cout  << "variablel a ="<< a << "variable b = "<< b <<endl;
}
int main(){
    int a,b;
    cout<<"variabel a=";
    cin>>a;
    cout<<"variable b=";
    cin>>b;
    cout<<"variable a="<< a <<"variable b="<< b << endl;
    tukar(a,b);
    return 0;
}    
